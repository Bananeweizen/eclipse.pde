<project name="Build Annotations JAR" default="build" basedir="..">

	<target name="build">
		<copy todir="${basedir}/lib">
			<fileset dir="${basedir}/../org.eclipse.pde.ds.lib" includes="*.jar,*.zip" />
		</copy>
	</target>

	<target name="clean">
		<delete dir="${basedir}/lib" includes="*.jar,*.zip" failonerror="false" />
	</target>
</project>